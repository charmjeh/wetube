extends layouts/main 
include mixins/videoBlock

block content
    // Added Mixin(Mixin/videoBlock) : Mixin is to recycle the same HTML structure and keep it organized on a different file.
    .videos
        each item in videos
            +videoBlock({ 
                id: item.id,
                title: item.title,
                views: item.views,
                videoFile:item.fileUrl
            })